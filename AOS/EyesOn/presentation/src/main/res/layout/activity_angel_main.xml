<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:tools="http://schemas.android.com/tools">

    <androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".view.angel.main.AngelMainActivity"
        android:background="@drawable/gradient_angel">

        <androidx.fragment.app.FragmentContainerView
            android:layout_marginTop="35dp"
            android:id="@+id/frame_angel_main"
            android:name="androidx.navigation.fragment.NavHostFragment"
            app:defaultNavHost="true"
            app:navGraph="@navigation/nav_graph_angel_main"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:layout="@layout/fragment_angel_main" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>